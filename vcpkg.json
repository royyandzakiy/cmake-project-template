{
	"$schema": "https://raw.githubusercontent.com/microsoft/vcpkg/master/scripts/vcpkg.schema.json",
	"name": "vcpkgcmakepresets",
	"version": "1.0",
	"description": "A C++ project demonstrating CMake Presets and Vcpkg integration.",
	"homepage": "https://github.com/your-username/vcpkgcmakepresets",
	"license": "MIT",
	"maintainers": [
		"Your Name <your.email@example.com>"
	],
	"dependencies": [
		{
			"name": "fmt",
			"version>=": "10.1.1"
		},
		{
			"name": "paho-mqttpp3"
		}
	],
	"features": {
		"tests": {
			"description": "Enable unit tests using Google Test",
			"dependencies": [
				{
					"name": "gtest",
					"version>=": "1.14.0"
				}
			]
		},
		"cppwinrt-feature": {
			"description": "Enable BLE functionality (Windows only)",
			"dependencies": [
				{
					"name": "cppwinrt",
					"platform": "windows"
				}
			],
			"supports": "windows"
		}
	},
	"default-features": [
		"tests"
	],
	"builtin-baseline": "8f63c9ed5897d7367a92a442a79f91721e0fd38f",
	"overrides": [
		{
			"name": "fmt",
			"version": "10.1.1"
		}
	]
}